{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.10"
  },
  "regions": ["sin1"],
  "functions": {
    "app.py": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}